using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace continue_statement_while_loop
{
    public class Program
    {
        public static void Main(string[] args)
        {
            int i = 1;
            while(i<=12)
            {
                i++;
                if(i==5)
                {
                    continue;
                }
                Console.WriteLine(i);
            }

        }
    }
}
